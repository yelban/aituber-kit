{
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "plugin:react-hooks/recommended"
  ],
  "plugins": [
    "prettier",
    "react-hooks"
  ],
  "rules": {
    "react-hooks/exhaustive-deps": "warn",
    "prettier/prettier": "error"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
